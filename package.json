{
  "name": "sms",
  "version": "0.0.1",
  "type": "module",
  "main": "./src/server.js",
  "scripts": {
    "start": "npm run db:migrate:latest && NODE_ENV=production node src/server.js",
    "dev": "node --watch ./src/server.js",
    "db:prepare:prod": "npm run db:migrate:latest",
    "db:rollback": "knex migrate:rollback --knexfile=./src/db/knexfile.js",
    "db:migrate:make": "knex migrate:make --knexfile=./src/db/knexfile.js",
    "db:migrate:latest": "knex migrate:latest --knexfile=./src/db/knexfile.js",
    "db:migrate:rollback": "knex migrate:rollback --knexfile=./src/db/knexfile.js",
    "db:seed:make": "knex seed:make --knexfile=./src/db/knexfile.js",
    "db:seed:run": "knex seed:run --knexfile=./src/db/knexfile.js",
    "rmds": "find . -name '.DS_Store' -delete"
  },
  "keywords": [
    "sms",
    "email"
  ],
  "author": "wajeht",
  "license": "MIT",
  "description": "find sms mail address of a phone number",
  "dependencies": {
    "better-sqlite3": "^11.7.2",
    "compression": "^1.7.5",
    "connect-flash": "^0.1.1",
    "connect-session-knex": "^5.0.0",
    "cors": "^2.8.5",
    "csrf-sync": "^4.0.3",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "knex": "^3.1.0"
  }
}
